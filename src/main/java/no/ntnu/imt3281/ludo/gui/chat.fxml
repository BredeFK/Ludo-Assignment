      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <TextArea fx:id="chatArea" editable="false" prefHeight="695.0" prefWidth="295.0" />
            <HBox alignment="TOP_RIGHT">
               <children>
                  <TextField fx:id="textToSay" prefHeight="26.0" prefWidth="316.0" />
                  <Button fx:id="sendTextButton" mnemonicParsing="false" prefWidth="67.0" text="%ludogameboard.saybutton" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
